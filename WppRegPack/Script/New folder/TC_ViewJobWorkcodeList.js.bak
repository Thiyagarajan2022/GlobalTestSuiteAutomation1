function getWorkCodeExcelDataForJobType(workcodeBook,workcode_sheetName,colName) { 
    excelData =[];
    var colsList = [];
    var xlDriver = DDT.ExcelDriver(workcodeBook, workcode_sheetName, true);
    var data = "";
    var i = 0;
    var name ;
     for(var idx=0;idx<DDT.CurrentDriver.ColumnCount;idx++){   
     colsList[idx] = DDT.CurrentDriver.ColumnName(idx);
     if(colsList[idx]==colName){ 
       name=colsList[idx];
       Log.Message("Name:"+name);
       break;
     }
   }
   
    while (!DDT.CurrentDriver.EOF()){
    data = "";
       
    temp = xlDriver.Value(name);
    if(temp !=null && temp.toString().trim().length>0){ 
        data =   xlDriver.Value(colsList[0]).toString().trim()+"*";
        data = data + xlDriver.Value(colsList[1]).toString().trim();
//        data = data + xlDriver.Value(name).toString().trim();
         Log.Message(data);
        excelData[i] = data;
      i++;
       }
     
       
     xlDriver.Next();
   }
    DDT.CloseDriver(xlDriver.Name);
//    Log.Message("completed reading excel data, data length::"+excelData.length);
//    return excelData;  
}



function compare(){ 
  
for(var i=0;i<excelData.length;i++){ 
var temp = true;
for(var j=0;j<wokList.length;j++){ 
   if(excelData[i]==wokList[j]){ 
     temp = false;
     break;
   }

}  
if(temp){ 
  Log.Warning("Missed in Maconomy but Available in ConfigPack :"+excelData[i]);
}
}

