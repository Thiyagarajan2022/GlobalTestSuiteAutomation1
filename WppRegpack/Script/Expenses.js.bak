    if(expenseDetail!=""){
      hsn.Click();
      SearchByValuePicker(hsn,"Local Specification 9",expenseDetail,"HSN")
      Delay(1000);
    } 
    else{
      ValidationUtils.verify(false,true,"HSN is Needed to Cteate a Expenses Sheet");
    } 
  }
    break;

  default:
    // code block
}
  
}



